{
    "pageBuilder": {
        "options": {
            "pageType": "objectexit",
            "selectors": {
                "recordEditor": ".csc-record-edit-container"
            },
            "components": {
                "recordEditor": {
                    "type":  "cspace.recordEditor",
                    "options": {
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "fieldsToIgnore": ["csid", "fields.csid", "fields.exitNumber"]
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            "fields.exitNumber",
                            "fields.currentOwner"
                        ]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "objectexit",
             "schema": [
                "recordlist",
                "namespaces",
                "recordtypes",
                "objectexit"
            ]
        }
    }
}
